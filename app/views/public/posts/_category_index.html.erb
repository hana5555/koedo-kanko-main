<!--部分テンプレート　カテゴリー検索機能-->
<div style="background-color:#FFF;">
  <div class="row">
    <table class="table table-borderless">
      <tr>
        <th class="text-center">
          <h5>カテゴリー</h5>
        </th>
      </tr>
      <% category.each do |category| %>
      <tr>
        <td class="text-center"><%= link_to category.name, posts_path(category_id: category.id) %></td>
      </tr>
      <% end %>
    </table>
  </div>
</div>